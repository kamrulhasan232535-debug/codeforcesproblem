#include <stdio.h>
int is_lucky(int num) {
    while (num > 0) {
        int digit = num % 10;
        if (digit != 4 && digit != 7) {
            return 0; 
        }
        num /= 10;
    }
    return 1; 
}
int is_almost_lucky(int n) {
    for (int i = 1; i <= n; i++) {
        if (is_lucky(i) && n % i == 0) {
            return 1; 
        }
    }
    return 0; 
}

int main() {
    int n;
    scanf("%d", &n);

    if (is_almost_lucky(n)) {
        printf("YES\n");
    } else {
        printf("NO\n");
    }

    return 0;
}
