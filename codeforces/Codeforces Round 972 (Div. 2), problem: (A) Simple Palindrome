#include <stdio.h>

int main() {
    char VOWELS[] = "aeiou";
    int t; 
    scanf("%d", &t);
    while (t--) {
        int n; 
        scanf("%d",&n);
        int v[5]={n/5,n/5,n/5,n/5,n/5}; 
        for (int i=0; i<n%5;i++) {
            v[i]++;
        }
        for (int i=0;i<5;i++) {
            for (int j=0;j<v[i];j++) {
                printf("%c",VOWELS[i]); 
            }
        }
        printf("\n");
    }
    return 0;
}
