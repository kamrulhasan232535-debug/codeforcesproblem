#include <stdio.h>
#include <math.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        long long n;
        scanf("%lld", &n);
        long long ans = 0;
        for (long long i = 1; i * i <= n; i++) {
            if (n % i == 0) {
                if (i < n){
                     ans += i;   
                }           
                long long j = n / i;
                if (j != i && j < n) {
                    ans += j;     
                }
            }
        }
        printf("%lld\n", ans);
    }
    return 0;
}
