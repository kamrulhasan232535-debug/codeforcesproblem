#include <stdio.h>
#include <stdlib.h>
int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        char pin[5];
        int array[4], sum= 0;
        scanf("%s", pin);
        for (int i = 0; i < 4; i++) {
            array[i] = (pin[i] == '0') ? 10 : (pin[i] - '0');
        }sum+= abs(array[0] - 1);
        for (int i = 1; i < 4; i++) {
            sum+= abs(array[i] - array[i - 1]);
        } printf("%d\n", sum+4);
    }
    return 0;
}
