import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        try( Scanner sc=new Scanner(System.in)){
            int t=sc.nextInt();
            while(t-->0){
                long n=sc.nextLong();
                long a=1;
                while(n>3){
                    n/=4;
                    a*=2;
                }System.out.println(a);
            }
        }
    }
}
