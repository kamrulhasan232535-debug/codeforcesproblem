#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    scanf("%s", s);

    char eq[] = "hello";
    int j = 0; 

    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] == eq[j]) {
            j++; 
        }
        if (j == 5) break; 
    }

    printf("%s\n", (j == 5) ? "YES" : "NO");
    return 0;
}
