#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int freq[100];
        for (int i = 0; i <=100; i++) {
            freq[i] = 0;
        }
        int array[n];
        int sum = 0;
        for (int i = 0; i < n; i++) {
            scanf("%d", &array[i]);
            if (array[i] >= 0 && array[i] <=100) {
                if (freq[array[i]] == 0) {
                    freq[array[i]] = 1;
                    sum++;
                }
            }
        }

        printf("%d\n", sum+(sum-1));
    }

    return 0;
}
