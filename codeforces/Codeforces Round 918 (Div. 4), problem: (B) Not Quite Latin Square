#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);  
    for (int testCase = 0; testCase < t; testCase++) {
        char grid[3][3];
        for (int i = 0; i < 3; i++) {
            scanf("%s", grid[i]);
        }
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                if (grid[i][j] == '?') {
                    char missing = 'A';
                    if (grid[i][0] != 'A' && grid[i][1] != 'A' && grid[i][2] != 'A') {
                        missing = 'A';
                    }
                    else if (grid[i][0] != 'B' && grid[i][1] != 'B' && grid[i][2] != 'B') {
                        missing = 'B';
                    }
                    else if (grid[i][0] != 'C' && grid[i][1] != 'C' && grid[i][2] != 'C') {
                        missing = 'C';
                    }
                    printf("%c\n", missing);
                    break;
                }
            }
        }
    }
    return 0;
}
