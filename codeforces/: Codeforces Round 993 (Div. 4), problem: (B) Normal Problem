#include <stdio.h>
#include <string.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        char s[101];
        scanf("%s", s);
        for (int i = 0; i < strlen(s); i++) {
            if (s[i] == 'p') {
                s[i] = 'q'; 
            } else if (s[i] == 'q') {
                s[i] = 'p'; 
            }
        }for(int i=strlen(s)-1; i>=0;i--){
            printf("%c",s[i]);
        }printf("\n");
    }
    return 0;
}
