#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    char t[1001];  
    scanf("%s", t);
    for (int i = 0; i < strlen(t); i += 2) {
        int c = i;  
        for (int j = i; j < strlen(t); j += 2) {
            if (t[c]-'0' > (t[j] - '0')) {  
                c = j;
            }
        }char temp=t[i];
        t[i]=t[c];
        t[c]=temp;
        
    }
    printf("%s\n",t);

    return 0;
}

