#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main(){
        char a[101],b[101];
        scanf("%s%s",&a,&b);
        int n=strlen(a),m=strlen(b);
        if(n!=m){
            printf("NO\n");
        }else{
            int flag=1;
            for(int i=n-1; i>=0; i--){
                if(a[i]!=b[n-1-i]){
                    flag=0;
                    break;
                }
            }printf("%s\n",(flag)?"YES":"NO");
        }
        
}
