#include <stdio.h>
#include <ctype.h>
#include <string.h>

int main() {
    char t[101], a[101];
    scanf("%s %s", t, a);
    for (int i = 0; i < strlen(t); i++) {
        t[i] = toupper(t[i]);
    }
    for (int i = 0; i < strlen(a); i++) {
        a[i] = toupper(a[i]);
    }
    int s = strcmp(t, a);
    if (s > 0) {
        printf("1\n");
    } else if (s < 0) {
        printf("-1\n");
    } else {
        printf("0\n");
    }

    return 0;
}
