#include <stdio.h>
#include <string.h>
int main() {
	char s[101];
	scanf("%s",s);
	int array[26]={0};
	for(int i=0; i<strlen(s); i++){
	    array[s[i]-'a']++;
	}int c=0;
	for(int i=0; i<26; i++){
	    if(array[i]>0){
	        c++;
	    }
	}printf("%s\n",(c%2==0)?"CHAT WITH HER!":"IGNORE HIM!");
	

}

