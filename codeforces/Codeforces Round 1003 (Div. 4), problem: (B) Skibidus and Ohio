#include <stdio.h>
#include <string.h>

int main() {
	int t;
	scanf("%d",&t);
	while(t--){
	    char s[1001];
	    scanf("%s",s);
	    int c=0;
	    for(int i=0; i<strlen(s)-1; i++){
	        if(s[i]==s[i+1]){
	            c=1;
	            break;
	        }
	    }printf("%d\n",(c==1)?1:strlen(s));
	}

}

