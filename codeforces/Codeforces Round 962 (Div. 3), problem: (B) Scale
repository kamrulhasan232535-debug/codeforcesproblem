import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int tc = sc.nextInt();

        while (tc-- > 0) {
            int n = sc.nextInt();
            int k = sc.nextInt();
            
            char[][] A = new char[n][n];
            
            for (int i = 0; i < n; i++) {
                String row = sc.next();
                for (int j = 0; j < n; j++) {
                    A[i][j] = row.charAt(j);
                }
            }
            
            for (int i = 0; i < n; i += k) {
                for (int j = 0; j < n; j += k) {
                    System.out.print(A[i][j]);
                }
                System.out.println();
            }
        }
        
        sc.close();
    }
}
