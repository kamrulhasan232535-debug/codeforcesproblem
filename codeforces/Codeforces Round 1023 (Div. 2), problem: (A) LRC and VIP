#include <stdio.h>

int gcd(int a, int b) {
    while (b) {
        int tmp = b;
        b = a % b;
        a = tmp;
    }
    return a;
}

int main() {
    int t;
    scanf("%d", &t);
    
    while (t--) {
        int n;
        scanf("%d", &n);
        int a[105], i, j;
        
        for (i = 0; i < n; ++i) {
            scanf("%d", &a[i]);
        }

        int found = 0;
        for (i = 0; i < n; ++i) {
            int g1 = a[i];
            int g2 = -1;
            for (j = 0; j < n; ++j) {
                if (j != i) {
                    if (g2 == -1) g2 = a[j];
                    else g2 = gcd(g2, a[j]);
                }
            }
            if (g1 != g2) {
                printf("YES\n");
                for (j = 0; j < n; ++j) {
                    if (j == i) printf("1 ");
                    else printf("2 ");
                }
                printf("\n");
                found = 1;
                break;
            }
        }

        if (!found) {
            printf("NO\n");
        }
    }
    
    return 0;
}
