#include <stdio.h>
#include <string.h>

int main() {
	int t;
	scanf("%d",&t);
	while(t--){
	    char s[1001];
	    scanf("%s",s);
	    if(strlen(s)>2){
	        for(int i=0; i<strlen(s)-2;i++){
	            printf("%c",s[i]);
	        }printf("%c\n",'i');
	    }else{
	        printf("i\n");
	    }
	    
	}

}

