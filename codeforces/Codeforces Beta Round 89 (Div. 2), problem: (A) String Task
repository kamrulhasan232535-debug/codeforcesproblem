#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char s[101], s1[101] = ""; 
    scanf("%s", s);
    int index = 0; 
    for (int i = 0; i < strlen(s); i++) {
        s[i] = tolower(s[i]); 
        if (s[i] == 'a' || s[i] == 'o' || s[i] == 'e' || s[i] == 'i' || s[i] == 'u' || s[i] == 'y') {
            continue;
        }
        s1[index++] = s[i]; 
    }
    s1[index] = '\0'; 
    for (int i = 0; i < strlen(s1); i++) {
        printf(".%c", s1[i]);
    }

    printf("\n");
    return 0;
}
