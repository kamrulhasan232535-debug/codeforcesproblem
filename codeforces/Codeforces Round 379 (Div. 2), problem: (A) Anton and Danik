#include<stdio.h>
#include<string.h>
int main(){
    int n;
    scanf("%d",&n);
    char s[n];
    scanf("%s",s);
    int c=0;
    int d=0;
    for(int i=0; i<strlen(s); i++){
        c+=(s[i]=='A')?1:0;
        d+=(s[i]=='D')?1:0;
    }printf("%s\n",(c>d)?"Anton":(d>c)?"Danik":"Friendship");
    
}
