#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    long long a= 0, b = 0, c= 0;
    int x;
    for(int i = 0; i < n; i++) {
        scanf("%d", &x);
        a+= x;
    }
    for(int i = 0; i < n-1; i++) {
        scanf("%d", &x);
        b+= x;
    }
    for(int i = 0; i < n-2; i++) {
        scanf("%d", &x);
        c+= x;
    }

    long long ans1 =a-b;   
    long long ans2 =b-c; 

    printf("%lld\n%lld\n", ans1,ans2);

    return 0;
}
