#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);  
    while (t--) {
        int n;
        scanf("%d", &n);
        int a[n], cnt[26] = {0};
        char s[n + 1]; 
        s[n] = '\0';  

        for (int i = 0; i < n; i++) {
            scanf("%d", &a[i]);  
        }

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < 26; j++) {
                if (cnt[j] == a[i]) {  
                    cnt[j]++;  
                    s[i] = 'a' + j;  
                    break;
                }
            }
        }

        printf("%s\n", s);  
    }

    return 0;
}
