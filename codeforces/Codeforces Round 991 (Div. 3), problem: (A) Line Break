#include <stdio.h>
#include<string.h>

int main() {
    int t;
    scanf("%d",&t);
    while(t--){
        int n,m,sum=0,sum2=0;
        scanf("%d %d",&n,&m);
        char s[10];
        for(int i=0; i<n; i++){
            scanf("%s",s);
            sum+=strlen(s);
            sum2+=(sum<=m)?1:0;
        }printf("%d\n",sum2);
    }

}

