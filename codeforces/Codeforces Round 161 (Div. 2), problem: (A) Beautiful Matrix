#include <stdio.h>
#include <math.h>

int main() {
    int array[5][5];
    int d, t;
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            scanf("%d", &array[i][j]); 
            if (array[i][j] == 1) {
                d = i;
                t = j;
            }
        }
    }
    printf("%d\n", abs(d - 2) + abs(t - 2));

    return 0;
}
