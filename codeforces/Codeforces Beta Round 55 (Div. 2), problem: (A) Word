#include <stdio.h>
#include <string.h>

int main() {
    char x[101];
    scanf("%s", x);
    int c=0;
    for(int i=0; i<strlen(x);i++){
        if(x[i]>='a' && x[i]<='z'){
            c++;
        }
    }if(strlen(x)-c>c){
        for(int i=0; i<strlen(x); i++){
            printf("%c",toupper(x[i]));
        }
    }else{
        for(int i=0; i<strlen(x); i++){
            printf("%c",tolower(x[i]));
        }
    }printf("\n");
    return 0;
}
