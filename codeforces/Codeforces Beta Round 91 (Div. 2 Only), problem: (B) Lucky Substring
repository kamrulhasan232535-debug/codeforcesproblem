#include <stdio.h>

int main() {
    char s[51];  
    scanf("%s", s);

    int count4 = 0, count7 = 0;
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] == '4') count4++;
        if (s[i] == '7') count7++;
    }

    if (count4 == 0 && count7 == 0) {
        printf("-1\n");
    }
    else if (count4 >= count7) {
        printf("4\n");
    } else {
        printf("7\n");
    }

    return 0;
}
