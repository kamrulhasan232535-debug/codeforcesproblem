import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        
        try( Scanner sc=new Scanner(System.in)){
            int t=sc.nextInt();
            while(t-->0){
                int n=sc.nextInt();
                int k=sc.nextInt();
                 for(int i=0; i<n;i++){
                     if(k==1){
                         System.out.print(i+1+" ");
                         
                     }else if(k==n){
                         System.out.print(1+" ");
                     }else{
                         System.out.print("-1");
                         
                         break;
                     }
                 }System.out.println();
                
            }
        }
    }
}
