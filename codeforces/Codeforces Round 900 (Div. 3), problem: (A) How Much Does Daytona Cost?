#include <stdio.h>
#include <string.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n, m;
        scanf("%d %d", &n, &m);
        int array[n];
        int c=0;
        for(int i=0; i<n; i++){
            scanf("%d ",&array[i]);
            c+=(array[i]==m)?1:0;
        }printf("%s\n",(c>0)?"YES":"NO");
    }
    return 0;
}
