#include <stdio.h>

int main() {
	int t;
	scanf("%d",&t);
	while(t--){
	    int n;
	    scanf("%d",&n);
	    for(int i=0;i<2*n;i++){
	        for(int j=0; j<2*n;j++){
	            if(((j/2)+(i/2))%2==0){
	                printf("#");
	            }else{
	                printf(".");
	            }
	        }printf("\n");
	    }
	}

}

