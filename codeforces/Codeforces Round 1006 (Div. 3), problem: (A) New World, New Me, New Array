#include <stdio.h>
#include <math.h>

int min(int n, int k, int p) {
    if (k < -n * p || k > n * p) return -1;
    return (int)ceil((double)abs(k) / p);
}

int main() {
    int t, n, k, p;
    scanf("%d", &t);
    while (t--) {
        scanf("%d %d %d", &n, &k, &p);
        printf("%d\n", min(n, k, p));
    }
    return 0;
}
