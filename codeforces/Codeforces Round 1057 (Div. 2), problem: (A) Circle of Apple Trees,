#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t--) {
        int n;
        scanf("%d", &n);

        int array[n];
        int freq[105] = {0}; 
        int count = 0;

        for (int i = 0; i < n; i++) {
            scanf("%d", &array[i]);
            if (!freq[array[i]]) {  
                freq[array[i]] = 1;
                count++;
            }
        }

        printf("%d\n", count);
    }

    return 0;
}
