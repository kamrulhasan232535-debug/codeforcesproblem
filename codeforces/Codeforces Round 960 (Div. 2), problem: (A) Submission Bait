#include <stdio.h>

#define N 55

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int q[N] = {0};
        
        for (int i = 0; i < n; ++i) {
            int x;
            scanf("%d", &x);
            q[x]++;
        }

        int found_odd = 0; 
        for (int i = 1; i <= n; ++i) {
            if (q[i] % 2 == 1) {
                found_odd = 1; 
                break;
            }
        }
        if (found_odd) {
            printf("YES\n");
        } else {
            printf("NO\n"); 
        }
    }
    return 0;
}
